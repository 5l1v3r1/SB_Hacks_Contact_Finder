{
  "name": "SB Hacks Contact Finder",
  "version": "2.0.0",
  "description": "Find contacs with ease",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js" : ["main.js"]
    }
  ],
  "page_action": {
    "default_icon": "images/icon.png",
    "default_popup": "sponsors.html"
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "tabs", "http://*/*", "https://*/*"
  ],
  "icons": { 
    "128" : "images/icon.png" 
  },
  "manifest_version": 2
}